    "@angular/language-service": "~13.2.0",
    "@datorama/akita-ngdevtools": "^7.0.0",
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.8.3",
    "@nrwl/cypress": "13.8.3",
    "@nrwl/eslint-plugin-nx": "13.8.3",
    "@nrwl/jest": "13.8.3",
