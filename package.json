{
  "name": "todo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "run-p start:todo start:mysql",
    "start:todo": "nx serve todo",
    "start:api": "docker run -it -p 3333:3333 --net=todo_default $(docker build -q .)",
    "start:mysql": "npm run mysql:start",
    "build": "nx build",
    "test": "nx test",
    "mysql:start": "docker compose --env-file=\".env\" up",
    "mysql:stop": "npm run mysql:dump:create && docker compose --env-file=\".env\" down",
    "mysql:dump:create": "docker exec mysql_container sh -c 'exec mysqldump --all-databases -uroot -p\"$MYSQL_ROOT_PASSWORD\"' > prisma/dump/all-databases.sql",
    "mysql:dump:restore": "docker exec -i mysql_container sh -c 'exec mysql -uroot -p\"$MYSQL_ROOT_PASSWORD\"' < prisma/dump/all-databases.sql",
    "prod:setup": "sh scripts/setup.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.2.5",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^13.2.5",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@auth0/auth0-angular": "^1.9.0",
    "@datorama/akita": "^7.1.1",
    "@datorama/akita-ng-entity-service": "^7.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.0.0",
    "@nrwl/angular": "13.8.3",
    "@prisma/client": "^3.10.0",
    "dotenv": "^16.0.0",
    "express-jwt": "^6.1.1",
    "jwks-rsa": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.0",
    "@angular-eslint/eslint-plugin": "~13.0.1",
